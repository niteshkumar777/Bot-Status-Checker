{
    "name": "Bot_Status_Checker",
    "logo": "https://telegra.ph/file/62bef61e158a5c7252d6d.jpg",
    "description": "Easy & powerful bot to check if your all Telegram bots are working or not. This bot status bot updates every 45 minutes & runs for 24x7 hours.",
    "repository": "https://github.com/PratheekXD/Bot_Status_Checker",
    "keywords": [
        "BotStatus",
        "How to check your bots are alive or not",
        "Up or down",
        "Check status of bots",
        "Telegram userbot",
        "Pyrogram userbot"
    ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "SESSION_STRING": {
            "description": "A valid Pyrogram session name",
            "required": true
        },
        "BOT_LIST": {
            "description": "Your bot username list without '@' (Example: pratheek1bot pratheek2bot pratheek3bot)",
            "required": true
        },
       "CHANNEL_OR_GROUP_ID": {
            "description": "Your channel's or group's Telegram id (Example: -1001246808642)",
            "required": true
        },
       "MESSAGE_ID": {
            "description": "Telegram id of message from your channel or group (Example: 10) Need Help Then Contact @Pratheek06",
            "required": true
        },
        "BOT_ADMIN_IDS": {
            "description": "Bot admin(s) id(s) (Example: 1356107531 2460197531 3579864241)"
        },
        "TIME_ZONE": {
            "description": "Your time zone (Example: Asia/Kolkata)",
            "required": true
        }
    },
    "formation": {
    "worker": {
      "quantity": 1,
      "size": "eco"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
